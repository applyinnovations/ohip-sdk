{
  "name": "@applyinnovations/ohip-sdk",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "reset-docs": "cd hospitality-api-docs && git reset --hard && cd ..",
    "clean": "find src -type f ! -name '*authenticationMiddleware.*' -exec rm {} + && find src -type d -empty -delete && rm -rf dist",
    "add-auth": "cp src/go/authenticationMiddleware.go dist/go/oauth/",
    "build": "openapi-generator-cli generate > build.log && yarn add-auth",
    "compile": "yarn tsc -p tsconfig.typescript-fetch.json"
  },
  "files": [
    "src"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/node": "^18.11.17",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
